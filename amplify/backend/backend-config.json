{
  "storage": {
    "Analyse": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "Users": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "createAnalyses": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "Analyse",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        },
        {
          "category": "storage",
          "resourceName": "Users",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        },
        {
          "category": "function",
          "resourceName": "appdevopsiimutils",
          "attributes": [
            "Arn"
          ]
        }
      ]
    },
    "createUser": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "Users",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        }
      ]
    },
    "appdevopsiimutils": {
      "providerPlugin": "awscloudformation",
      "service": "LambdaLayer",
      "build": true
    },
    "getUserAnalysis": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "appdevopsiimutils",
          "attributes": [
            "Arn"
          ]
        },
        {
          "category": "storage",
          "resourceName": "Analyse",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        },
        {
          "category": "storage",
          "resourceName": "Users",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        }
      ]
    }
  },
  "auth": {
    "appdevopsiim": {
      "service": "Cognito",
      "serviceType": "managed",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "frontendAuthConfig": {
        "socialProviders": [],
        "usernameAttributes": [],
        "signupAttributes": [
          "EMAIL"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyMinLength": 8,
          "passwordPolicyCharacters": []
        },
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      }
    }
  },
  "api": {
    "analyses": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "createAnalyses",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "getUserAnalysis",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  }
}